{
  "name": "electric-testcase-01",
  "version": "0.1.0",
  "author": "ElectricSQL",
  "type": "module",
  "scripts": {
    "backend:start": "docker compose start",
    "backend:stop": "docker compose stop",
    "backend:up": "docker compose up --detach",
    "backend:down": "docker compose down",
    "client:generate": "yarn electric-sql generate --service http://localhost:5133 --proxy postgresql://prisma:postgres@localhost:65432/tc01",
    "client:watch": "yarn electric-sql generate --watch",
    "db:migrate": "yarn pg-migrations apply --database postgresql://postgres:postgres@localhost:65432/tc81 --directory ./db/migrations",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "wa-sqlite": "github:rhashimoto/wa-sqlite",
    "@electric-sql/prisma-generator": "1.1.1",
    "@prisma/client": "^4.8.1",
    "electric-sql": "0.8.1",
    "zod": "3.21.1"
  },
  "devDependencies": {
    "@databases/pg-migrations": "^5.0.3",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "prisma": "4.8.1"
  },
  "resolutions": {
    "zod": "3.21.1"
  }
}
